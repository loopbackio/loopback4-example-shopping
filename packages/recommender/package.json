{
  "name": "loopback4-example-recommender",
  "version": "1.1.1",
  "description": "Mock up product recommendation",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node .",
    "docker:run": "docker run -p 3001:3001 -p 50051:50051 -d recommender"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback4-example-shopping.git",
    "directory": "packages/recommender"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@grpc/proto-loader": "0.5.5",
    "@loopback/http-server": "2.1.12",
    "express": "4.17.1",
    "grpc": "1.24.3"
  },
  "devDependencies": {
    "@loopback/build": "6.2.1",
    "@loopback/testlab": "3.2.3",
    "@types/express": "4.17.7",
    "@types/node": "12.12.54",
    "typescript": "3.9.7"
  },
  "copyright.owner": "IBM Corp."
}
