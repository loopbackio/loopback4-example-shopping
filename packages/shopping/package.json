{
  "name": "loopback4-example-shopping",
  "version": "1.1.1",
  "description": "LoopBack 4 Example: Online Shopping APIs",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist* *.tsbuildinfo",
    "migrate": "node ./dist/migrate",
    "pretest:ci": "npm run build",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "test:ci": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "concurrently --kill-others \"npm run start:app\" \"npm run start:recommender\"",
    "start:app": "node .",
    "start:recommender": "node ../recommender",
    "prepublishOnly": "npm run test",
    "docker:run": "docker run -p 3000:3000 -d shopping"
  },
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "3.2.2",
    "@loopback/authorization": "0.4.4",
    "@loopback/boot": "1.5.11",
    "@loopback/context": "1.23.5",
    "@loopback/core": "1.10.7",
    "@loopback/openapi-v3": "1.10.1",
    "@loopback/repository": "1.15.4",
    "@loopback/rest": "1.23.0",
    "@loopback/rest-explorer": "1.4.4",
    "@loopback/security": "0.1.7",
    "@loopback/service-proxy": "1.3.11",
    "@types/jsonwebtoken": "8.3.2",
    "bcryptjs": "2.4.3",
    "casbin": "3.0.6",
    "debug": "4.1.1",
    "express": "4.17.1",
    "isemail": "3.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "loopback-connector-grpc": "1.3.0",
    "loopback-connector-kv-redis": "3.0.2",
    "loopback-connector-mongodb": "5.2.0",
    "loopback-connector-rest": "3.6.0"
  },
  "devDependencies": {
    "@loopback/build": "2.0.16",
    "@loopback/testlab": "1.9.4",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.2",
    "@types/lodash": "4.14.146",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.7",
    "concurrently": "5.0.0",
    "loopback4-example-recommender": "1.1.1",
    "mocha": "6.2.2",
    "source-map-support": "0.5.16",
    "typescript": "3.7.2"
  },
  "copyright.owner": "IBM Corp."
}
